<!DOCTYPE html>
<html>
  <head>
    <title><%= t "title" %></title>
    <%= csrf_meta_tags %>
    <%= stylesheet_link_tag :application_admin, media: "all" %>
    <%= javascript_include_tag :application_admin %>
  </head>
  <body ng-app="adminApp">
    <header>
      <div id="wrapper">
        <%= render "layouts/admin/header" %>
        <%= render "layouts/admin/left_navigation" %>
        <div id="page-wrapper">
          <div class="container-fluid">
            <div class="row">
              <div class="col-lg-12">
                <%= yield %>
              </div>
            </div>
          </div>
        </div>
      </div>
      <!-- <section class="footer">
      </section> -->
    </header>
    <%= javascript_include_tag :ng_admin %>
  </body>
</html>
